[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/cran-to-git
	shallow = true
	branch = HEAD
[submodule "airr"]
	path = airr
	url = https://github.com/cran/airr
	shallow = true
	registered = true
[submodule "alakazam"]
	path = alakazam
	url = https://bitbucket.org/kleinstein/alakazam
	shallow = true
	registered = true
[submodule "shazam"]
	path = shazam
	url = https://bitbucket.org/kleinstein/shazam
	shallow = true
	registered = true
